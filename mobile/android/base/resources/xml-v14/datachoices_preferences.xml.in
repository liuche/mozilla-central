<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
         xmlns:gecko="http://schemas.android.com/apk/res/@ANDROID_PACKAGE_NAME@"
         android:enabled="false">
    <SwitchPreference android:key="toolkit.telemetry.enabled"
                      android:title="@string/datachoices_telemetry_title"
                      android:summary="@string/datachoices_telemetry_summary" />

    <SwitchPreference android:key="android.not_a_preference.healthreport.uploadEnabled"
                      android:title="@string/datachoices_fhr_title"
                      android:summary="@string/datachoices_fhr_summary"
                      android:defaultValue="true" />

    <SwitchPreference android:key="datanotification.crashreporter.submitEnabled"
                      android:title="@string/datachoices_crashreporter_title"
                      android:summary="@string/datachoices_crashreporter_summary"
                      android:defaultValue="false" />
</PreferenceScreen>
